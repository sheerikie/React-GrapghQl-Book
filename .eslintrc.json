{
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },

    "env": {
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },

    "rules" : {
        "no-unused-vars" : 2,
        "no-undef" : 2
    },
    "extends": [
        "plugin:prettier/recommended"
    ]
    
}
